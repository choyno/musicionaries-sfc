<%= render "components/song-search" %>
<%= render "components/song-list" %>

<% if @clps.count > 0 %>
  <div class="row">
    <% @clps.each do |clp| %>
      <div class="col s12 m4">
        <%= render partial: "components/clp-details", locals: { clp: clp } %>
      </div>
    <% end %>
  </div>
<% end %>